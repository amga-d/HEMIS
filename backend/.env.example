# Replace these with your actual Supabase database credentials

# Database URL for Prisma (Connection Pooling)
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-ID].supabase.co:6543/postgres?pgbouncer=true&connection_limit=1"

# Direct URL for Prisma (Direct Connection) - Optional
DIRECT_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-ID].supabase.co:5432/postgres"

NODE_ENV=development

PORT=3000

RATE_LIMIT_WINDOW=900000 # 15 minutes

RATE_LIMIT_MAX=100

ALLOWED_ORIGINS="https://yourdomain.com,https://www.yourdomain.com:http://localhost:3001"

JWT_SECRET="JWT_SECRET"

# Gemini AI Configuration
GEMINI_API_KEY="Insert your API Key"

